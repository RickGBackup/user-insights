
<h1>Registered App - <%= @registered_app.name %> </h1>
<br/>
<h3>URL: <%= @registered_app.url %>  </h3>
<h3>Events </h3>
<ul>
<% @events.each do |event, instances| %>
<li><%= "#{event} - #{instances.count}"%></li>
<% end %>
</ul>

<%= link_to 'Return to My Apps', registered_apps_path %> - <%= link_to 'Delete this App', registered_app_path(@registered_app), method: :delete %>
